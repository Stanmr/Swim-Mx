<mapper namespace="db.consultasSql">
	<select id="todosCargos" resultType="Cargo">
		SELECT id_cargo, cargo, descripcion
		FROM cargos;
	</select>
	
	<select id="unCargo" resultType="Cargo">
		SELECT id_cargo, cargo, descripcion
		FROM cargos WHERE ;
	</select>
</mapper>
	
